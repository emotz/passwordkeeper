{
  "name": "passwordkeeper",
  "version": "0.1.0",
  "description": "Simple storage for passwords",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "nprogress": "^0.2.0",
    "passport": "^0.3.2",
    "toastr": "^2.1.2",
    "vue": "^2.2.4",
    "vue-bootstrap-modal": "github:eoksni/vue-bootstrap-modal#905ce6b16992b238e8b9394976e9b2b049400cbb",
    "vue-i18n": "^5.0.3",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "optionalDependencies": {
    "@types/jasmine": "^2.5.47",
    "eslint": "^3.19.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "inject-loader": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "wdio-jasmine-framework": "^0.3.0",
    "wdio-selenium-standalone-service": "0.0.8",
    "webdriverio": "^4.6.2"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babili-webpack-plugin": "0.0.11",
    "bootstrap-webpack": "0.0.6",
    "copy-webpack-plugin": "^4.0.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "local-web-server": "^1.2.7",
    "null-loader": "^0.1.1",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0",
    "webpack-runner": "^1.3.0"
  },
  "scripts": {
    "dev": "cd frontend && cd server-mocks && ws",
    "backend": "nodemon -L --watch backend backend/src/main.js",
    "build": "cd frontend && webpack --config vendor.webpack.config.js && webpack",
    "build:watch": "cd frontend && webpack --config vendor.webpack.config.js && webpack-runner --config=webpack.config.js --watch",
    "watch": "parallelshell 'npm run dev -s' 'npm run build:watch -s' 'npm run backend -s'",
    "test-unit": "cd frontend && webpack --config test.webpack.config && karma start",
    "test-unit:watch": "npm run test-unit && parallelshell 'npm run test-unit:watch-build -s' 'npm run test-unit:watch-run -s'",
    "test-unit:watch-run": "cd frontend && karma start --single-run=false --auto-watch",
    "test-unit:watch-build": "cd frontend && webpack-runner --config=test.webpack.config.js --watch",
    "test-e2e": "cd e2e && wdio",
    "test": "npm run test-unit && npm run test-e2e",
    "lint": "eslint . --format unix",
    "clean": "rimraf bin frontend/dist .vscode/chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emotz/passwordkeeper.git"
  },
  "keywords": [
    "password",
    "keeper"
  ],
  "author": "emotz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emotz/passwordkeeper/issues"
  },
  "homepage": "https://github.com/emotz/passwordkeeper#readme"
}
