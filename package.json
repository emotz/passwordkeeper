  "scripts": {
    "watch:dev": "cd fe && ws",
    "build": "cd fe && webpack",
    "build:vendor": "cd fe && webpack --config vendor.webpack.config.js",
    "watch:build": "cd fe && webpack-runner --config=webpack.config.js --watch",
    "test:unit": "cd fe && cd tests && webpack && karma start",
    "watch:build:test:unit": "cd fe && cd tests && webpack-runner --config=webpack.config.js --watch",
    "watch:test:unit": "cd fe && cd tests && karma start --single-run=false --auto-watch",
    "test:e2e": "wdio",
    "nobuild:test:unit": "cd fe && cd tests && karma start",
    "test": "npm run test:unit && npm run test:e2e",
    "postinstall": "cd fe/tests && typings install",
    "clean": "rimraf bin errorShots fe/dist fe/tests/dist .vscode/chrome",
    "clean:dist": "rimraf fe/dist fe/tests/dist",
    "watch:backend": "nodemon --debug=0.0.0.0:5858 -L src/main.js"
  },
  "repository": {
    "type": "git",
