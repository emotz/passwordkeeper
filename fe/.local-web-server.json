{
    "port": 8000,
    "rewrite": [
        {
            "from": "/*",
            "to": "/dist/$1"
        }
    ],
    "spa": "src/index.html",
    "mocks": [
        {
            "route": "/dist/entries/:id",
            "module": "/mocks/entry.js"
        },
        {
            "route": "/dist/entries",
            "module": "/mocks/entries.js"
        }
    ]
}