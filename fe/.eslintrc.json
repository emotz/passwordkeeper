{
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "commonjs": true,
        "jquery": true,
        "browser": true
    },
    "globals": {
        "process": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2
    },
    "plugins": [
        "async-await",
        "vue"
    ]
}