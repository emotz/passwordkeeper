{
    "env": {
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2
    },
    "plugins": [
        "async-await"
    ]
}